export declare class <%- identifier %> {
  constructor (api: any)
<%_ for (const method of methods) { _%>

  /**
  <%_ if (method.description) { _%>
   * <%- method.description %>
  <%_ } _%>
   * @see https://bungie-net.github.io/#<%- method.swagger.operationId %>
   *
   <%_ for (const arg of method.arguments) { _%>
   * @param <%- compileJSDocArgumentType(arg) %> <%- compileJSDocArgument(arg) %> <%- arg.description %>
   <%_ } _%>
   * @returns {Promise}
   */
  <%- method.identifier %> (<%- method.arguments.map(compileTypeScriptArgument).join(', ') %>): Promise<any>
<%_ } _%>
}
